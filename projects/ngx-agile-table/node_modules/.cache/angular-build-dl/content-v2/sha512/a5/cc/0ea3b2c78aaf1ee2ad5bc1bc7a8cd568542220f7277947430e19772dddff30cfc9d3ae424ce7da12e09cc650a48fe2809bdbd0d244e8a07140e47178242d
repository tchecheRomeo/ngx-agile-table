class ActionButtonTable {
    /**
     * @param {?} key
     * @param {?=} text
     * @param {?=} icon
     * @param {?=} tooltip
     * @param {?=} html
     */
    constructor(key, text, icon, tooltip, html) {
        this.key = key;
        this.icon = icon || '';
        this.text = text || '';
        this.tooltip = tooltip || this.text;
        this.html = html || '';
        this.target = -1;
        this.displayConditionFn = (/**
         * @param {?} data
         * @return {?}
         */
        data => true);
    }
    /**
     * @param {?} value
     * @return {?}
     */
    keyValue(value) {
        this.key = value;
        return this;
    }
    /**
     * @param {?} value
     * @return {?}
     */
    iconValue(value) {
        this.icon = value || '';
        return this;
    }
    /**
     * @param {?} value
     * @return {?}
     */
    textValue(value) {
        this.text = value || '';
        return this;
    }
    /**
     * @param {?} value
     * @return {?}
     */
    tooltipValue(value) {
        this.tooltip = value || '';
        return this;
    }
    /**
     * @param {?} value
     * @return {?}
     */
    htmlValue(value) {
        this.html = value || '';
        return this;
    }
    /**
     * @param {?} condition
     * @return {?}
     */
    displayCondition(condition) {
        this.displayConditionFn = condition;
        return this;
    }
}
