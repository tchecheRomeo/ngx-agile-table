if (false) {}
class NgxAgileTableComponent {
    constructor() {
        this.actionButtons = [];
        this.collapseActionButton = new ActionButtonTable('collapse', '...');
        this.columnSettings = [];
        this.data = [];
        this.emptyText = 'No content';
        this.actionButtonTitle = '';
        this.searchText = 'Search';
        this.maxSizeText = 'Max size';
        this.totalElementsText = 'Total Elements';
        this.paginationPreviousText = '<<';
        this.paginationNextText = '>>';
        this.paginationPreviousIcon = '';
        this.paginationNextIcon = '';
        this.displayGlobalSearchInput = false;
        this.colorVariationLineBreak = 2;
        this.maxActionButtonPerRow = 3;
        this.collapseActionButtonPosition = 'right';
        this.height = '';
        this.totalElements = 0;
        this.totalPages = 0;
        this.elementPerPageList = [];
        this.enableColumnFiltering = true;
        this.enableColumnSorting = true;
        this.localPagination = false;
        this.onClickActionButton = new _angular_core__WEBPACK_IMPORTED_MODULE_0__["EventEmitter"]();
        this.onPageChanged = new _angular_core__WEBPACK_IMPORTED_MODULE_0__["EventEmitter"]();
        this.onElementPerPageChanged = new _angular_core__WEBPACK_IMPORTED_MODULE_0__["EventEmitter"]();
        this.onGlobalSearchRequest = new _angular_core__WEBPACK_IMPORTED_MODULE_0__["EventEmitter"]();
        this.onRowDisplayed = new _angular_core__WEBPACK_IMPORTED_MODULE_0__["EventEmitter"]();
        this.pageSelected = 1;
        this.collapseActionButtonPositions = ['right', 'bottom'];
        this.pagesRangeLimit = 5;
        this.pageStart = 1;
        this.globalSearchKeywords = '';
        this.columnSearchKeyword = '';
        this.columnSearchAttr = '';
        this.columnSortDirectionAsc = false;
        this.columnSortAttr = '';
        this.rows = [];
        this.rowsToDisplay = [];
    }
    /**
     * @return {?}
     */
    ngOnInit() {
        if (!this.data) { // Value passed is null
            this.data = [];
        }
        if (this.totalElements === 0 && this.data.length !== 0) {
            this.totalElements = this.data.length;
        }
        if (!this.elementPerPage) {
            this.elementPerPage = this.data.length;
        }
        this.displayData();
    }
    /**
     * @return {?}
     */
    formatRows() {
        /** @type {?} */
        const rows = [];
        /** @type {?} */
        let row;
        /** @type {?} */
        let canDisplayButton;
        for (const dt of this.data) {
            row = new Row();
            row.data = dt;
            row.actionButtons = [];
            row.collapsedActionButtons = [];
            for (const actionButton of this.actionButtons) {
                canDisplayButton = actionButton.displayConditionFn(this.data.find((/**
                 * @param {?} d
                 * @return {?}
                 */
                d => JSON.stringify(dt) === JSON.stringify(d))));
                if (canDisplayButton) {
                    if (row.actionButtons.length < this.maxActionButtonPerRow - 1) { // -1 for collapse button
                        row.actionButtons.push(actionButton);
                    }
                    else {
                        row.collapsedActionButtons.push(actionButton);
                    }
                }
            }
            rows.push(row);
        }
        this.rows = rows;
    }
    /**
     * @param {?} changes
     * @return {?}
     */
    ngOnChanges(changes) {
        if (changes.data) {
            this.data = changes.data.currentValue;
        }
        if (changes.actionButtons) {
            this.actionButtons = changes.actionButtons.currentValue;
        }
        if (changes.collapseActionButton) {
            this.collapseActionButton = changes.collapseActionButton.currentValue;
        }
        if (changes.collapseActionButtonPosition) {
            this.collapseActionButtonPosition = changes.collapseActionButtonPosition.currentValue;
        }
        if (changes.maxActionButtonPerRow) {
            this.maxActionButtonPerRow = changes.maxActionButtonPerRow.currentValue;
        }
        if (changes.totalElements) {
            this.totalElements = changes.totalElements.currentValue;
        }
        if (changes.totalPages) {
            this.totalPages = changes.totalPages.currentValue;
        }
        if (changes.elementPerPage) {
            this.elementPerPage = changes.elementPerPage.currentValue;
        }
        if (changes.height) {
            this.height = changes.height.currentValue;
        }
        if (changes.columnSettings) {
            this.columnSettings = changes.columnSettings.currentValue;
        }
        if (changes.colorVariationLineBreak) {
            this.colorVariationLineBreak = changes.colorVariationLineBreak.currentValue;
        }
        if (changes.displayGlobalSearchInput) {
            this.displayGlobalSearchInput = changes.displayGlobalSearchInput.currentValue;
        }
        if (changes.elementPerPageList) {
            this.elementPerPageList = changes.elementPerPageList.currentValue;
        }
        if (changes.localPagination) {
            this.localPagination = changes.localPagination.currentValue;
        }
        if (changes.enableColumnFiltering) {
            this.enableColumnFiltering = changes.enableColumnFiltering.currentValue;
        }
        if (changes.enableColumnSorting) {
            this.enableColumnSorting = changes.enableColumnSorting.currentValue;
        }
        if (changes.emptyText) {
            this.emptyText = changes.emptyText.currentValue;
        }
        if (changes.maxSizeText) {
            this.maxSizeText = changes.maxSizeText.currentValue;
        }
        if (changes.totalElementsText) {
            this.totalElementsText = changes.totalElementsText.currentValue;
        }
        if (this.elementPerPageList) {
            if (this.elementPerPageList.length === 0) {
                this.elementPerPageList = [this.elementPerPage, 20, 50, 100, 200];
            }
            if (this.elementPerPageList.indexOf(this.elementPerPage) === -1) {
                this.elementPerPageList.push(this.elementPerPage);
            }
            this.elementPerPageList.sort((/**
             * @param {?} a
             * @param {?} b
             * @return {?}
             */
            (a, b) => a < b ? -1 : 1));
        }
        this.displayData();
    }
    /**
     * @return {?}
     */
    displayData() {
        this.formatRows();
        if (this.localPagination) {
            this.totalPageCalculation();
            this.rowsToDisplay = this.segmentation(1);
        }
        else {
            this.rowsToDisplay = [].concat(this.rows);
        }
    }
    /**
     * @param {?=} data
     * @return {?}
     */
    totalPageCalculation(data) {
        data = data ? data : this.rows;
        if (data) {
            this.totalPages = data.length / this.elementPerPage;
            if ((data.length % this.elementPerPage) !== 0 || this.totalPages === 0) {
                this.totalPages++;
            }
        }
    }
    /**
     * @param {?=} data
     * @return {?}
     */
    resetTotalPages(data) {
        this.pageSelected = 1;
        this.pageStart = 1;
        if (this.localPagination) {
            this.totalPageCalculation(data);
        }
    }
    /**
     * @param {?} page
     * @param {?=} data
     * @return {?}
     */
    segmentation(page, data) {
        data = data ? data : this.rows;
        if (data) {
            if (page === 1) {
                return data.slice(0, this.elementPerPage);
            }
            else if (data.length >= this.elementPerPage * page) {
                return data.slice((page - 1) * this.elementPerPage, page * this.elementPerPage);
            }
            else {
                return data.slice((page - 1) * this.elementPerPage, this.rows.length);
            }
        }
        return [];
    }
    /**
     * @return {?}
     */
    pages() {
        /** @type {?} */
        const result = [];
        /** @type {?} */
        const presumePageEnd = this.pageStart + this.pagesRangeLimit;
        /** @type {?} */
        const limit = presumePageEnd <= this.totalPages ? presumePageEnd - 1 : this.totalPages;
        for (let i = this.pageStart; i <= limit; i++) {
            result.push(i);
        }
        return result;
    }
    /**
     * @param {?} button
     * @param {?} index
     * @return {?}
     */
    onActionButtonClicked(button, index) {
        if (!this.collapseActionButton || this.collapseActionButton.key !== button.key) {
            button.targetData = this.rowsToDisplay[index].data;
            button.target = this.rows.findIndex((/**
             * @param {?} r
             * @return {?}
             */
            r => JSON.stringify(button.targetData) === JSON.stringify(r.data)));
            this.onClickActionButton.emit(button);
        }
    }
    /**
     * @return {?}
     */
    globalSearchRequest() {
        this.onGlobalSearchRequest.emit(this.globalSearchKeywords);
    }
    /**
     * @param {?} page
     * @return {?}
     */
    onPageChange(page) {
        if (this.localPagination) {
            if (this.columnSearchKeyword && this.columnSearchKeyword !== '') {
                this.filterTable(this.columnSearchAttr, this.columnSearchKeyword, page);
            }
            else if (this.columnSortAttr && this.columnSortAttr !== '') {
                this.sortDataDisplay(this.columnSortAttr, this.columnSortDirectionAsc, page);
            }
            else {
                this.rowsToDisplay = this.segmentation(page);
            }
        }
        else {
            this.onPageChanged.emit(page);
        }
        this.pageSelected = page;
    }
    /**
     * @return {?}
     */
    onElementPerPageChange() {
        if (this.localPagination) {
            if (this.columnSearchKeyword && this.columnSearchKeyword !== '') {
                this.filterTable(this.columnSearchAttr, this.columnSearchKeyword, 1);
            }
            else if (this.columnSortAttr && this.columnSortAttr !== '') {
                this.sortDataDisplay(this.columnSortAttr, this.columnSortDirectionAsc, 1);
            }
            else {
                this.resetTotalPages();
                this.rowsToDisplay = this.segmentation(1);
            }
        }
        else {
            this.onElementPerPageChanged.emit(this.elementPerPage);
        }
    }
    /**
     * @param {?} attr
     * @param {?} keyword
     * @param {?=} page
     * @return {?}
     */
    filterTable(attr, keyword, page) {
        /** @type {?} */
        let searchResults;
        if (keyword && keyword !== '') {
            searchResults = this.rows.filter((/**
             * @param {?} r
             * @return {?}
             */
            r => (this.columnValue(r.data, attr, false) + '').toLowerCase().includes(keyword.toLowerCase())));
        }
        else {
            searchResults = this.rows;
        }
        this.rowsToDisplay = this.segmentation(page ? page : 1, searchResults);
        this.resetTotalPages(searchResults);
        this.columnSearchKeyword = keyword;
        this.columnSearchAttr = attr;
    }
    /**
     * @param {?} attr
     * @param {?} ascFiltering
     * @param {?=} page
     * @return {?}
     */
    sortDataDisplay(attr, ascFiltering, page) {
        /** @type {?} */
        let searchResults;
        if (ascFiltering) {
            searchResults = this.rows.sort((/**
             * @param {?} a
             * @param {?} b
             * @return {?}
             */
            (a, b) => {
                /** @type {?} */
                let value = this.columnValue(a.data, attr, false);
                /** @type {?} */
                let value2 = this.columnValue(b.data, attr, false);
                value = parseFloat(value) ? parseFloat(value) : value;
                value2 = parseFloat(value2) ? parseFloat(value2) : value2;
                if (value < value2) {
                    return 1;
                }
                if ((value > value2)) {
                    return -1;
                }
                return 0;
            }));
        }
        else {
            searchResults = this.rows.sort((/**
             * @param {?} a
             * @param {?} b
             * @return {?}
             */
            (a, b) => {
                /** @type {?} */
                let value = this.columnValue(a.data, attr, false);
                /** @type {?} */
                let value2 = this.columnValue(b.data, attr, false);
                value = parseFloat(value) ? parseFloat(value) : value;
                value2 = parseFloat(value2) ? parseFloat(value2) : value2;
                if (value > value2) {
                    return 1;
                }
                if ((value < value2)) {
                    return -1;
                }
                return 0;
            }));
        }
        this.rowsToDisplay = this.segmentation(page ? page : 1, searchResults);
        this.resetTotalPages(searchResults);
        this.columnSortDirectionAsc = ascFiltering;
        this.columnSortAttr = attr;
    }
    /**
     * @param {?} attr
     * @param {?} sortColumnDirection
     * @return {?}
     */
    sortByColumn(attr, sortColumnDirection) {
        this.sortDataDisplay(attr, sortColumnDirection.checked);
        sortColumnDirection.checked = !sortColumnDirection.checked;
    }
    /**
     * @return {?}
     */
    nextPages() {
        if (this.pageStart + this.pagesRangeLimit - 1 < this.totalPages) {
            this.pageStart += this.pagesRangeLimit;
        }
    }
    /**
     * @return {?}
     */
    previousPages() {
        if (this.pageStart > this.pagesRangeLimit) {
            this.pageStart -= this.pagesRangeLimit;
        }
    }
    /**
     * @param {?} data
     * @param {?} columnProperty
     * @return {?}
     */
    tableDataTitleAttribute(data, columnProperty) {
        /** @type {?} */
        const value = '' + this.columnValue(data, columnProperty, false);
        if (value.includes('<') && value.includes('>')) {
            return '';
        }
        else {
            return value;
        }
    }
    /**
     * @param {?} data
     * @param {?} columnProperty
     * @param {?=} sanitize
     * @return {?}
     */
    columnValue(data, columnProperty, sanitize) {
        /** @type {?} */
        let value;
        if (columnProperty.includes('.')) {
            /** @type {?} */
            let desiredValue = JSON.parse(JSON.stringify(data));
            columnProperty.split('.').forEach((/**
             * @param {?} element
             * @return {?}
             */
            (element) => {
                if (element.includes('[') && element.includes(']')) {
                    /** @type {?} */
                    const openHookIndex = element.indexOf('[');
                    /** @type {?} */
                    const closeHookIndex = element.indexOf(']');
                    desiredValue = desiredValue[element.substring(0, openHookIndex)][parseInt(element.substring(openHookIndex + 1, closeHookIndex), 10)];
                }
                else if (desiredValue) {
                    desiredValue = desiredValue[element];
                }
            }));
            value = desiredValue;
        }
        else {
            value = data[columnProperty];
        }
        if (sanitize !== false) {
            return this.sanitize(value);
        }
        else {
            return (value !== null && value !== undefined) ? value : '';
        }
    }
    /**
     * @param {?} data
     * @param {?} columnTable
     * @return {?}
     */
    columnDisplayValue(data, columnTable) {
        /** @type {?} */
        const originalValue = this.columnValue(data, columnTable.nameProperty, false);
        /** @type {?} */
        const displayValue = columnTable.customCellFn(originalValue, data);
        if (displayValue.length !== 0) { // If not default value : Default value returned by customCellFn() is empty string
            return this.sanitize(displayValue);
        }
        else {
            return originalValue;
        }
    }
    /**
     * @param {?} value
     * @return {?}
     */
    sanitize(value) {
        return value ? NgxAgileTableService.getSanitizer().bypassSecurityTrustHtml(value) : '<span></span>';
    }
    /**
     * @param {?} button
     * @param {?} index
     * @return {?}
     */
    displayButton(button, index) {
        return button.displayConditionFn(this.rows.find((/**
         * @param {?} d
         * @return {?}
         */
        d => JSON.stringify(this.rowsToDisplay[index].data) === JSON.stringify(d.data))));
    }
    /**
     * @return {?}
     */
    getCollapsePosition() {
        if (this.collapseActionButtonPositions.indexOf(this.collapseActionButtonPosition) !== -1) {
            return 'collapse-' + this.collapseActionButtonPosition;
        }
        else {
            return 'collapse-' + this.collapseActionButtonPositions[0];
        }
    }
    /**
     * @param {?} data
     * @return {?}
     */
    getActionButtonToDisplay(data) {
        return this.rows.find((/**
         * @param {?} row
         * @return {?}
         */
        row => row.data === data)).actionButtons;
    }
    /**
     * @param {?} data
     * @return {?}
     */
    getActionButtonToCollapse(data) {
        return this.rows.find((/**
         * @param {?} row
         * @return {?}
         */
        row => row.data === data)).collapsedActionButtons;
    }
    /**
     * @param {?} data
     * @param {?} nativeElement
     * @return {?}
     */
    rowDisplayed(data, nativeElement) {
        this.onRowDisplayed.emit(new RowTable(data, nativeElement));
        return '';
    }
}
